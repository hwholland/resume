<core:View xmlns:core="sap.ui.core" xmlns="sap.ui.comp.smartchart"
	xmlns:html="http://www.w3.org/1999/xhtml" xmlns:sl="sap.ui.comp.navpopover"
	controllerName="sap.ui.comp.sample.personalization.example9.Example">

	<SmartChart enableAutoBinding="true" entitySet="ProductCollection"
		useVariantManagement="true" persistencyKey="PKeyChartExample9"
		useChartPersonalisation="true" header="ProductCollection"
		showFullScreenButton="true" selectionMode="Multi" showChartTooltip="true">
		<semanticObjectController>
			<sl:SemanticObjectController
				navigationTargetsObtained="onNavigationTargetsObtained" navigate="onNavigate" />
		</semanticObjectController>
	</SmartChart>
</core:View>
