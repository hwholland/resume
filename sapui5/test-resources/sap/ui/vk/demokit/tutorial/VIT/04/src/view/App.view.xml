<mvc:View
		controllerName="viewportScenetree.controller.App"
		xmlns="sap.m"
		xmlns:mvc="sap.ui.core.mvc"
		xmlns:vk="sap.ui.vk"
		xmlns:l="sap.ui.layout"
		xmlns:f="sap.ui.layout.form"
		xmlns:u="sap.ui.unified"
		displayBlock="true">
	<App id="viewportScenetree">
		<Page
				title="{i18n>pageTitle}">
			<vk:Viewport
					id="viewport"
					width="100%"
					height="25%"/>
			<vk:SceneTree
					id="scenetree"
					width="100%"
					height="25%"/>
		</Page>
	</App>
</mvc:View>