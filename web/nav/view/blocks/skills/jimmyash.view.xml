<mvc:View xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:mvc="sap.ui.core.mvc">
	<l:BlockLayout background="Dashboard" class="sapUiLargeMarginBegin">
		<l:BlockLayoutRow>
			<l:BlockLayoutCell>
				<Title text="Technical Skills"/>
			</l:BlockLayoutCell>
		</l:BlockLayoutRow>
		<l:BlockLayoutRow>
			<l:BlockLayoutCell>
				<List backgroundDesign="Transparent" items="{ path: 'tokens&gt;/jimmyash'}">
					<items>
						<StandardListItem description="{tokens&gt;description}" info="{tokens&gt;level}" title="{tokens&gt;text}"/>
					</items>
				</List>
			</l:BlockLayoutCell>
		</l:BlockLayoutRow>
	</l:BlockLayout>
</mvc:View>