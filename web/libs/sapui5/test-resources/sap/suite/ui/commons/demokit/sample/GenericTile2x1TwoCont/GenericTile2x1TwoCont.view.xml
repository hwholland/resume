<core:View
	controllerName="sap.suite.ui.commons.sample.GenericTile2x1TwoCont.GenericTile2x1TwoCont"
	xmlns="sap.suite.ui.commons"
	xmlns:core="sap.ui.core">
	<GenericTile 
		header="Comparative Annual Totals"
		subheader="Expenses By Region"
		size="S" 
		frameType="TwoByOne"
		press="press">
		<tileContent>
			<TileContent footer="Actual and Target" unit="EUR" size="S">
				<content>
					<NumericContent size="S" scale="M" value="1.96" valueColor="Error" indicator="Up"/>
				</content>
			</TileContent>
			<TileContent footer="Compare across regions" unit="EUR" size="S">
				<content>
					<ComparisonChart size="S" scale="M">
				        <data>
				        	<ComparisonData	title="Americas" value="234" color="Good"/>
				        	<ComparisonData	title="EMEA" value="97" color="Error"/>
				        	<ComparisonData	title="APAC" value="197" color="Critical"/>
				        </data>
			        </ComparisonChart>
				</content>
			</TileContent>
		</tileContent>
	</GenericTile>
	<GenericTile 
		header="Comparative Annual Totals"
		subheader="Expenses By Region"
		size="M" 
		frameType="TwoByOne"
		press="press">
		<tileContent>
			<TileContent footer="Actual and Target" unit="EUR" size="M">
				<content>
					<NumericContent size="M" scale="M" value="1.96" valueColor="Error" indicator="Up"/>
				</content>
			</TileContent>
			<TileContent footer="Compare across regions" unit="EUR" size="M">
				<content>
					<ComparisonChart size="M" scale="M">
				        <data>
				        	<ComparisonData	title="Americas" value="234" color="Good"/>
				        	<ComparisonData	title="EMEA" value="97" color="Error"/>
				        	<ComparisonData	title="APAC" value="197" color="Critical"/>
				        </data>
			        </ComparisonChart>
				</content>
			</TileContent>
		</tileContent>
	</GenericTile>
</core:View>
