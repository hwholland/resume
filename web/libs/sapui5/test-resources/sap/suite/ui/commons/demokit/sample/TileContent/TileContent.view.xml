<core:View
	xmlns="sap.suite.ui.commons"
	xmlns:core="sap.ui.core">
	<TileContent footer="Actual and Target" unit="EUR" size="S"	class="marginTopLeft">
		<content>
			<BulletChart size="S" scale="M" targetValue="75" targetValueLabel="75c" minValue="0" maxValue="150">
				<actual>
					<BulletChartData value="125" color="Error"/>
				</actual>
				<thresholds>
					<BulletChartData value="35" color="Critical"/>
					<BulletChartData value="115" color="Error"/>
				</thresholds>
			</BulletChart> 
		</content>
	</TileContent>
	<TileContent footer="Current Quarter" unit="EUR" size="S" class="marginTopLeft">
		<content>
			<NumericContent size="S" scale="M" value="1.96" valueColor="Error" indicator="Up"/>
		</content>
	</TileContent>
	<TileContent size="S" class="marginTopLeft">
		<content>
			<ComparisonChart size="S" scale="M">
		        <data>
		        	<ComparisonData	title="Americas" value="234" color="Good"/>
		        	<ComparisonData	title="EMEA" value="97" color="Error"/>
		        	<ComparisonData	title="APAC" value="197" color="Critical"/>
		        </data>
	        </ComparisonChart>
		</content>
	</TileContent>
	<TileContent size="M" footer="Leave Requests" class="marginTopLeft">
		<content>
			<NumericContent size="M" value="3" icon="sap-icon://travel-expense" />
		</content>
	</TileContent>
	<TileContent size="M" footer="Hours since last Activity" class="marginTopLeft">
		<content>
			<NumericContent size="M" value="9" icon="sap-icon://locked" />
		</content>
	</TileContent>
	<TileContent footer="New Notifications" size="M" class="marginTopLeft">
		<content>
			<JamContent 
				size="M" 
				contentText="@@notify Great outcome of the Presentation today. The new functionality and the new design was well received." 
				subheader="about 1 minute ago in Computer Market"
				value="132"/> 
		</content>
	</TileContent>
	<TileContent footer="August 21, 2013" size="M" class="marginTopLeft">
		<content>
			<NewsContent 
				size="M" 
				contentText="SAP Unveils Powerful New Player Comparison Tool Exclusively on NFL.com" 
				subheader="SAP News"/> 
		</content>
	</TileContent>
</core:View>