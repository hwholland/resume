<core:View
	controllerName="sap.suite.ui.commons.sample.ChartContainer.ChartContainer"
	xmlns="sap.suite.ui.commons"
	xmlns:m="sap.m"
	xmlns:vbm="sap.ui.vbm"
	xmlns:mvc="sap.ui.core.mvc" 
	xmlns:viz="sap.viz.ui5.controls"
	xmlns:layout="sap.ui.layout"
	xmlns:core="sap.ui.core"
	height="100%">
	<layout:Splitter width="100%" height="100%">
	<layout:contentAreas>
	<ChartContainer
		id="idChartContainer"
		showFullScreen="true"
		showPersonalization="true"
		autoAdjustHeight="true"
		showLegend="true"
		personalizationPress="attachPersonalizationPress"
		contentChange="attachContentChange">

		<dimensionSelectors>
			<m:Select id="idoSelect1"> </m:Select> 
			<m:Select id="idoSelect2"> </m:Select>
			<m:Select id="idoSelect3"> </m:Select>
		</dimensionSelectors>

		<customIcons>
			<core:Icon id="idoIcon1" width="2em"> </core:Icon> 
			<core:Icon id="idoIcon2" width="2em"> </core:Icon> 
			<core:Icon id="idoIcon3" width="2em"> </core:Icon> 
		</customIcons>
		
		<content>
			<ChartContainerContent
			   	icon = "sap-icon://horizontal-bar-chart" 
				title = "vizFrame Bar Chart Sample"> 
				<content>
					<viz:VizFrame id="idoVizFrame" uiConfig="{applicationSet:'fiori'}"> </viz:VizFrame>
				</content>
			</ChartContainerContent>	
			<ChartContainerContent
			   	icon = "sap-icon://line-chart" 
				title = "vizFrame Line Chart Sample"> 
				<content>
					<viz:VizFrame id="idoVizFrame2"> </viz:VizFrame>
				</content>
			</ChartContainerContent>			
			<ChartContainerContent
			   	icon = "sap-icon://bubble-chart" 
				title = "vizFrame Bubble Chart Sample"> 
				<content>
					<viz:VizFrame id="idoVizFrame3" uiConfig="{applicationSet:'fiori'}"> </viz:VizFrame>
				</content>
			</ChartContainerContent>		
			<ChartContainerContent
			   	icon = "sap-icon://vertical-bar-chart" 
				title = "vizFrame Column Chart Sample"> 
				<content>
					<viz:VizFrame id="idoVizFrame4"> </viz:VizFrame>
				</content>
			</ChartContainerContent>	
			<ChartContainerContent
			   	icon = "sap-icon://horizontal-stacked-chart" 
				title = "vizFrame Stacked Bar Chart Sample"> 
				<content>
					<viz:VizFrame id="idoVizFrame5" uiConfig="{applicationSet:'fiori'}"> </viz:VizFrame>
				</content>
			</ChartContainerContent>
			<ChartContainerContent
			   	icon = "sap-icon://vertical-stacked-chart" 
				title = "vizFrame Stacked Column Chart Sample"> 
				<content>
					<viz:VizFrame id="idoVizFrame6" uiConfig="{applicationSet:'fiori'}"> </viz:VizFrame>
				</content>
			</ChartContainerContent>
			<ChartContainerContent
			   	icon = "sap-icon://business-objects-experience" 
				title = "vizFrame Combination Chart Sample"> 
				<content>
					<viz:VizFrame id="idoVizFrame7" uiConfig="{applicationSet:'fiori'}"> </viz:VizFrame>
				</content>
			</ChartContainerContent>
			<ChartContainerContent
			   	icon = "sap-icon://horizontal-bar-chart-2" 
				title = "vizFrame Bar Chart Sample"> 
				<content>
					<viz:VizFrame id="idoVizFrame8" uiConfig="{applicationSet:'fiori'}"> </viz:VizFrame>
				</content>
			</ChartContainerContent>
			<ChartContainerContent
			   	icon = "sap-icon://table-chart" 
				title = "Table Sample"> 
				<content>
					<m:Table id="idoTable"> </m:Table>
				</content>
			</ChartContainerContent>
			<ChartContainerContent
			   	icon = "sap-icon://map" 
				title = "AnalyticMap Sample"> 
				<content>
				<vbm:AnalyticMap 
			        id="vbi"
			        width="100%"
			        regions ="{/regionProperties}">
			        <vbm:regions>
			        <vbm:Region           
			            code="{code}" 
			            color="{color}"
			            tooltip = "{tooltip}"/>
			      </vbm:regions>
			      <vbm:legend>
			        <vbm:Legend
			            id="legend"
			            caption ="Analytic Legend"
			            items ="{/regionProperties}">
			            <vbm:LegendItem           
			                text="{legend}" 
			                color="{color}"
			                tooltip = "{tooltip}"/>
			        </vbm:Legend>
			      </vbm:legend>
			    </vbm:AnalyticMap>

				</content>
			</ChartContainerContent>		
		</content>	
	</ChartContainer>
	</layout:contentAreas>
	</layout:Splitter>	
</core:View>