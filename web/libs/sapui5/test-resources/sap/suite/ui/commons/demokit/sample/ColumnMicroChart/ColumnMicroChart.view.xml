<core:View
	controllerName="sap.suite.ui.commons.sample.ColumnMicroChart.ColumnMicroChart"
	xmlns="sap.suite.ui.commons"
	xmlns:m="sap.m"
	xmlns:core="sap.ui.core">
	<ColumnMicroChart size="M" press="press" class="marginTopLeft">
    	<columns>
			<ColumnData	value="100" color="Error"/>
			<ColumnData	value="60" color="Neutral"/>
		</columns>
    </ColumnMicroChart>
	<ColumnMicroChart size="M" press="press" class="marginTopLeft">
    	<columns>
			<ColumnData	value="60" color="Error"/>
			<ColumnData	value="80" color="Error"/>
			<ColumnData	value="100" color="Error"/>
			<ColumnData	value="60" color="Neutral"/>
			<ColumnData	value="55" color="Neutral"/>
			<ColumnData	value="15" color="Neutral"/>
		</columns>
    </ColumnMicroChart>
	<ColumnMicroChart size="S" press="press" class="marginTopLeft">
    	<columns>
			<ColumnData	value="60" color="Error"/>
			<ColumnData	value="100" color="Error"/>
			<ColumnData	value="-20" color="Error"/>
			<ColumnData	value="60" color="Neutral"/>
			<ColumnData	value="55" color="Neutral"/>
			<ColumnData	value="15" color="Neutral"/>
		</columns>
    </ColumnMicroChart>
	<ColumnMicroChart size="S" press="press" class="marginTopLeft">
    	<columns>
			<ColumnData	value="65" color="Error"/>
			<ColumnData	value="90" color="Error"/>
			<ColumnData	value="75" color="Error"/>
			<ColumnData	value="95" color="Error"/>
			<ColumnData	value="100" color="Error"/>
			<ColumnData	value="90" color="Error"/>
			<ColumnData	value="80" color="Error"/>
			<ColumnData	value="35" color="Neutral"/>
			<ColumnData	value="65" color="Neutral"/>
			<ColumnData	value="55" color="Neutral"/>
			<ColumnData	value="60" color="Neutral"/>
			<ColumnData	value="5" color="Neutral"/>
			<ColumnData	value="15" color="Neutral"/>
			<ColumnData	value="15" color="Neutral"/>
		</columns>
    </ColumnMicroChart>
	<ColumnMicroChart size="S" press="press" class="marginTopLeft">
    	<columns>
			<ColumnData	value="60" color="Good"/>
			<ColumnData	value="100" color="Good"/>
			<ColumnData	value="-20" color="Error"/>
			<ColumnData	value="60" color="Good"/>
			<ColumnData	value="55" color="Neutral"/>
			<ColumnData	value="15" color="Critical"/>
		</columns>
    </ColumnMicroChart>
</core:View>