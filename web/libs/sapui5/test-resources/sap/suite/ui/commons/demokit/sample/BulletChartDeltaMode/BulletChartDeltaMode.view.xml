<core:View
	controllerName="sap.suite.ui.commons.sample.BulletChartDeltaMode.BulletChartDeltaMode"
	xmlns="sap.suite.ui.commons"
	xmlns:core="sap.ui.core">
		<BulletChart size="XS" scale="M" targetValue="100" forecastValue="110" class="marginTopLeft" mode="Delta" showDeltaValue="true" showValueMarker="false" press="press">
			<actual>
				<BulletChartData value="120" color="Good"/>
			</actual>
			<thresholds>
				<BulletChartData value="0" color="Error"/>
				<BulletChartData value="50" color="Critical"/>
				<BulletChartData value="150" color="Critical"/>
				<BulletChartData value="200" color="Error"/>
