<core:View
	controllerName="sap.suite.ui.commons.sample.MicroAreaChart.MicroAreaChart"
	xmlns="sap.suite.ui.commons"
	xmlns:m="sap.m"
	xmlns:core="sap.ui.core">
	<MicroAreaChart width="164px" height="74px" minXValue="0" maxXValue="100" minYValue="0" maxYValue="100" class="marginTopLeft" press="press">
   		<firstXLabel>
   			<MicroAreaChartLabel label="June 1" color="Good"/>
   		</firstXLabel>
   		<lastXLabel>
   			<MicroAreaChartLabel label="June 30" color="Critical"/>
   		</lastXLabel>
   		<firstYLabel>
   			<MicroAreaChartLabel label="0M" color="Good"/>
   		</firstYLabel>
   		<lastYLabel>
   			<MicroAreaChartLabel label="80M" color="Critical"/>
   		</lastYLabel>
   		<chart>
   			<MicroAreaChartItem>
   				<points>
	    			<MicroAreaChartPoint x="0" y="0" />
	    			<MicroAreaChartPoint x="30" y="20" />
	    			<MicroAreaChartPoint x="60" y="20" />
	    			<MicroAreaChartPoint x="100" y="80" />
    			</points>
   			</MicroAreaChartItem>
   		</chart>
   		<target>
   			<MicroAreaChartItem>
   				<points>
	    			<MicroAreaChartPoint x="0" y="0" />
	    			<MicroAreaChartPoint x="30" y="30" />
	    			<MicroAreaChartPoint x="60" y="40" />
	    			<MicroAreaChartPoint x="100" y="90" />
    			</points>
   			</MicroAreaChartItem>
   		</target>
   		<maxThreshold>
   			<MicroAreaChartItem color="Good">
   				<points>
    			 	<MicroAreaChartPoint x="0" y="0" />
	    			<MicroAreaChartPoint x="30" y="40" />
	    			<MicroAreaChartPoint x="60" y="50" />
	    			<MicroAreaChartPoint x="100" y="100" />
    			</points>
   			</MicroAreaChartItem>
   		</maxThreshold>
   		<minThreshold>
   			<MicroAreaChartItem color="Error">
   				<points>
	    			<MicroAreaChartPoint x="0" y="0" />
	    			<MicroAreaChartPoint x="30" y="20" />
	    			<MicroAreaChartPoint x="60" y="30" />
	    			<MicroAreaChartPoint x="100" y="70" />
    			</points>
   			</MicroAreaChartItem>
   		</minThreshold>
       </MicroAreaChart>
	<MicroAreaChart width="164px" height="42px" class="marginTopLeft" press="press">
   		<chart>
   			<MicroAreaChartItem>
   				<points>
	    			<MicroAreaChartPoint x="0" y="0" />
	    			<MicroAreaChartPoint x="30" y="20" />
	    			<MicroAreaChartPoint x="60" y="20" />
	    			<MicroAreaChartPoint x="100" y="80" />
    			</points>
   			</MicroAreaChartItem>
   		</chart>
   		<target>
   			<MicroAreaChartItem>
   				<points>
	    			<MicroAreaChartPoint x="0" y="0" />
	    			<MicroAreaChartPoint x="100" y="55" />
    			</points>
   			</MicroAreaChartItem>
   		</target>
       </MicroAreaChart>
	<MicroAreaChart width="164px" height="74px" minXValue="0" maxXValue="100" minYValue="0" maxYValue="100" class="marginTopLeft" press="press">
   		<firstXLabel>
   			<MicroAreaChartLabel label="June 1" color="Good"/>
   		</firstXLabel>
   		<lastXLabel>
   			<MicroAreaChartLabel label="June 30" color="Good"/>
   		</lastXLabel>
   		<firstYLabel>
   			<MicroAreaChartLabel label="30M" color="Good"/>
   		</firstYLabel>
   		<lastYLabel>
   			<MicroAreaChartLabel label="70M" color="Good"/>
   		</lastYLabel>
   		<minLabel>
   			<MicroAreaChartLabel label="25M" color="Critical"/>
   		</minLabel>
   		<maxLabel>
   			<MicroAreaChartLabel label="70M" color="Good"/>
   		</maxLabel>
   		<chart>
   			<MicroAreaChartItem>
   				<points>
	    			<MicroAreaChartPoint x="0" y="30" />
	    			<MicroAreaChartPoint x="20" y="45" />
	    			<MicroAreaChartPoint x="40" y="40" />
	    			<MicroAreaChartPoint x="60" y="25" />
	    			<MicroAreaChartPoint x="80" y="65" />
	    			<MicroAreaChartPoint x="100" y="70" />
    			</points>
   			</MicroAreaChartItem>
   		</chart>
   		<target>
   			<MicroAreaChartItem>
   				<points>
	    			<MicroAreaChartPoint x="0" y="40" />
	    			<MicroAreaChartPoint x="100" y="60" />
    			</points>
   			</MicroAreaChartItem>
   		</target>
   		<maxThreshold>
   			<MicroAreaChartItem color="Error">
   				<points>
	    			<MicroAreaChartPoint x="0" y="60" />
	    			<MicroAreaChartPoint x="100" y="100" />
    			</points>
   			</MicroAreaChartItem>
   		</maxThreshold>
   		<minThreshold>
   			<MicroAreaChartItem color="Error">
   				<points>
	    			<MicroAreaChartPoint x="0" y="20" />
	    			<MicroAreaChartPoint x="100" y="10" />
    			</points>
   			</MicroAreaChartItem>
   		</minThreshold>
   		<innerMaxThreshold>
   			<MicroAreaChartItem color="Good">
   				<points>
	    			<MicroAreaChartPoint x="0" y="50" />
	    			<MicroAreaChartPoint x="100" y="80" />
    			</points>
   			</MicroAreaChartItem>
   		</innerMaxThreshold>
   		<innerMinThreshold>
   			<MicroAreaChartItem color="Good">
   				<points>
	    			<MicroAreaChartPoint x="0" y="30" />
	    			<MicroAreaChartPoint x="100" y="30" />
    			</points>
   			</MicroAreaChartItem>
   		</innerMinThreshold>
	</MicroAreaChart>
</core:View>