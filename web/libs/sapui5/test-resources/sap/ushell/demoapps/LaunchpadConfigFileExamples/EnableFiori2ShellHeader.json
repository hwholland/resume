          "applications": {
            "Shell-home": {
              "enableNotificationsPreview": false,
              "enableEasyAccess": false,
              "enableActionModeMenuButton": false,
              "enableActionModeFloatingButton": false,
              "enableTileActionsIcon": false,
              "enableHideGroups": false
