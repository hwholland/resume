<mvc:View
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns="sap.m"
	xmlns:vbm="sap.ui.vbm"
   height="100%"
   displayBlock="true">
	<vbm:GeoMap id="vbi" width="100%" height="100%">
	    <vbm:resources>
		    <vbm:Resource 
		        name="pin_yellow.png"
		        value="">
		    </vbm:Resource>
		    <vbm:Resource 
		        name="pin_blue.png"
		        src="test-resources/sap/ui/vbm/demokit/media/images/pin_blue.png">
		    </vbm:Resource>
		</vbm:resources>
		<vbm:vos>
		    <vbm:Boxes>
				<vbm:Box 
				    scale="0.1;0.1;0.1"
				    position="-40;50;0" 
				    tooltip="This is a Box" 
				    color="RGB(0,255,0)"
				    click="onClickItem" 
				    contextMenu="onContextMenuItem"/>
			</vbm:Boxes>
			<vbm:Spots>
				<vbm:Spot 
				    id="pin_yellow"
				    position="-50;0;0" 
				    image="pin_yellow.png"
				    click= "onClickItem" 
				    contextMenu= "onContextMenuItem"/>
				<vbm:Spot 
				    id="pin_blue"
				    position="0;0;0" 
				    image="pin_blue.png"
				    click= "onClickItem" 
				    contextMenu= "onContextMenuItem"/>
			</vbm:Spots>
	    </vbm:vos>			  
	    <vbm:legend>
			<vbm:Legend
			    id="legend"
			    caption ="Legend">
					<vbm:LegendItem 	        
					    text="Box" 
					    color="RGB(0,255,0)"
					    tooltip = "tooltip"/>
					<vbm:LegendItem 	        
					    text="pin_blue" 
					    image="pin_blue.png"
					    tooltip = "tooltip"/>
					<vbm:LegendItem 	        
					    text="pin_yellow" 
					    image="pin_yellow.png"
					    tooltip = "tooltip"/>
			</vbm:Legend>
		</vbm:legend>
	</vbm:GeoMap>
</mvc:View>