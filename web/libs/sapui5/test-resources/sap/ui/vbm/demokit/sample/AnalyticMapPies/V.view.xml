<mvc:View
	controllerName="sap.ui.vbm.sample.AnalyticMapPies.C"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns="sap.suite.ui.commons"
  	xmlns:m="sap.m"
	xmlns:vbm="sap.ui.vbm"
   height="100%"
   displayBlock="true">
	<vbm:AnalyticMap 
	    id="vbi"
	    width="100%"
	    height="100%"
	    regionClick="onRegionClick"
		regionContextMenu="onRegionContextMenu"
		regions ="{/regionProperties}">
	    <vbm:regions>
			<vbm:Region 	        
			    code="{code}" 
			    color="{color}"
			    tooltip = "{tooltip}"/>
		</vbm:regions>
		<vbm:Pies>
			<vbm:Pie 
			    scale="1;1;1"
			    position="0;0;0" 
			    click="onClickItem" 
			    contextMenu="onContextMenuItem"
			    items="{/PieSeries}" >
				<vbm:PieItem
				    value="{value}"
				    tooltip="{tooltip}"/>
			</vbm:Pie>
		</vbm:Pies> 
	</vbm:AnalyticMap>
</mvc:View>