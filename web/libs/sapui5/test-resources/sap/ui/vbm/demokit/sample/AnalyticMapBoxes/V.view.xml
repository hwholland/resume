<mvc:View
	controllerName="sap.ui.vbm.sample.AnalyticMapBoxes.C"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns="sap.suite.ui.commons"
  	xmlns:m="sap.m"
	xmlns:vbm="sap.ui.vbm"
   height="100%"
   displayBlock="true">
	<vbm:AnalyticMap 
	    id="vbi"
	    width="100%"
	    height="100%"
	    regionClick="onRegionClick"
		regionContextMenu="onRegionContextMenu"
		regions ="{/regionProperties}">
	    <vbm:regions>
			<vbm:Region 	        
			    code="{code}" 
			    color="{color}"
			    tooltip = "{tooltip}"/>
		</vbm:regions>
		<vbm:Boxes>
			<vbm:Box 
			    scale="0.1;0.1;0.1"
			    position="-40;50;0" 
			    tooltip="This is a Box" 
			    color="RGB(0,255,0)"
			    click="onClickItem" 
			    contextMenu="onContextMenuItem"/>
		</vbm:Boxes>
	</vbm:AnalyticMap>
</mvc:View>