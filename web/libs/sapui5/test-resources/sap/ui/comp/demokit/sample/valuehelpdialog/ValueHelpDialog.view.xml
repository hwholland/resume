<mvc:View
	height="100%"
	controllerName="sap.ui.comp.sample.valuehelpdialog.ValueHelpDialog"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:m="sap.m"
	xmlns:c="sap.ui.core"
	xmlns="sap.ui.comp.variants">
	
	<l:Grid defaultSpan="L6 M6 S6" class="gridMarginTop">
    <l:content>
	  	<f:SimpleForm 
	        minWidth="1024"
	        maxContainerCols="2"
	        editable="true"
	        layout="ResponsiveGridLayout"
	        labelSpanL="3"
	        labelSpanM="3"
	        emptySpanL="4"
	        emptySpanM="4"
	        columnsL="1"
	        columnsM="1"
	        class="editableForm">
	        <f:content>
				<m:Label text="Company"/>
				<m:MultiInput id="multiInput" valueHelpRequest="onValueHelpRequest"/> 		
			</f:content>
	    </f:SimpleForm>
	</l:content>
    </l:Grid> 	
</mvc:View>