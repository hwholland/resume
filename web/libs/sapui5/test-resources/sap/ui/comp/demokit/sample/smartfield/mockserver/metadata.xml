					<PropertyRef Name="CATC" />
				</Key>
				<Property Name="CATC" Type="Edm.String" Nullable="false"
					MaxLength="4" sap:display-format="UpperCase" sap:label="Category"
					sap:text="LTXT" />
				<Property Name="LTXT" Type="Edm.String" Nullable="false"
					sap:label="Category Description" />
			</EntityType>
			<EntityContainer Name="com.sap.GL.zrha_Entities"
				m:IsDefaultEntityContainer="true" sap:supported-formats="atom json xlsx">
				<EntitySet Name="Products" EntityType="com.sap.GL.zrha.Product"
					sap:creatable="false" sap:updatable="true" sap:deletable="false"
					sap:pageable="false" sap:content-version="1" />
				<EntitySet Name="VL_SH_H_TCURC" EntityType="com.sap.GL.zrha.VL_SH_H_TCURC"
