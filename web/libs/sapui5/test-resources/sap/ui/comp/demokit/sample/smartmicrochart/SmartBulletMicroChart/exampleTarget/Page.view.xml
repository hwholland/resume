<core:View xmlns:core="sap.ui.core" xmlns="sap.ui.comp.smartmicrochart" xmlns:l="sap.ui.layout" xmlns:m="sap.m" controllerName="sap.ui.comp.sample.smartmicrochart.SmartBulletMicroChart.exampleTarget.Page">
	<m:Page id="page" title="SmartBulletMicroChart criticality calculation target direction" enableScrolling="false">
		<m:Panel>
			<l:VerticalLayout>
				<l:content>
					<m:Label text="Positive" />
					<SmartBulletMicroChart id="targetSmartChart" entitySet="Products" />

					<m:Label text="Critical" />
					<SmartBulletMicroChart id="targetSmartChartWarningLow" entitySet="Products" />

					<m:Label text="Critical" />
					<SmartBulletMicroChart id="targetSmartChartWarningHigh" entitySet="Products" />

					<m:Label text="Negative" />
					<SmartBulletMicroChart id="targetSmartChartErrorLow" entitySet="Products" />

					<m:Label text="Negative" />
					<SmartBulletMicroChart id="targetSmartChartErrorHigh" entitySet="Products" />
				</l:content>
			</l:VerticalLayout>
		</m:Panel>
	</m:Page>
</core:View>