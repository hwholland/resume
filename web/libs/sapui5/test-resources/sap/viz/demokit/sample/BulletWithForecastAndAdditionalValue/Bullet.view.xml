<mvc:View height="100%" width="100%"
	controllerName="sap.viz.sample.BulletWithForecastAndAdditionalValue.Bullet"
	xmlns:viz="sap.viz.ui5.controls" 
	xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m">
    <ScrollContainer height="100%" width="100%" horizontal="false" vertical="true" focusable="true">
       <viz:Popover id="idPopOver"></viz:Popover>
       <viz:VizFrame id="idVizFrameBullet_with_forecast_and_additional_value" uiConfig="{applicationSet:'fiori'}" vizType="bullet" width="100%" height="500px">
       </viz:VizFrame>
       <VBox>
        <RadioButton id = "idRadio1" groupName="group_bullet" text="Horizontal" selected="true"/>
        <RadioButton id = "idRadio2" groupName="group_bullet" text="Vertical" />
       </VBox>
    </ScrollContainer>
</mvc:View>