<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog id="dataLossDialog" title="{i18n>WARNING}" state="Warning" type="Message">
		<content>
			<Text id="dataLossText" text="{i18n>DATA_LOSS_MESSAGE}" />
		</content>
		<buttons>
			<Button id="dataLossOKButton" text="{i18n>OK}"
				press="onDataLossOK" />
			<Button id="dataLossCancelButton" text="{i18n>CANCEL}"
				press="onDataLossCancel" />
		</buttons>
	</Dialog>
</core:FragmentDefinition>