<Dialog xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc"
	xmlns:suite="sap.suite.ui.commons" xmlns="sap.m" id='addTileDialog' title="{i18n>CURRENT_EVALUATION_MINI_CHARTS}"
	class="addTileDialog" height="100px" contentHeight="100%" contentWidth="auto" 
	horizontalScrolling='false' verticalScrolling='true'>
	<content>
		<List id='tileContainer'></List>
	</content>
	<beginButton>
		<Button text="{i18n>OK}" press="onAddTileOk" />
	</beginButton>
	<endButton>
		<Button text="{i18n>CANCEL}" press="onAddTileCancel" />
	</endButton>
</Dialog>
