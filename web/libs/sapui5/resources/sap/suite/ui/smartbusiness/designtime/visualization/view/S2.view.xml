<core:View xmlns:core="sap.ui.core" xmlns="sap.m"
	controllerName="sap.suite.ui.smartbusiness.designtime.visualization.view.S2">
	<Page id="page" title="{i18n>MASTER_TITLE}">
		<content>
			<List id="list" 
			items="{
				path: '/EVALUATIONS_MODELER',
				sorter: {
	        		path: 'MANUAL_ENTRY',
	        		descending: true,
	        		group: '.formatGroupName'
	      		},
	      		parameters: {'expand':'TAGS'}
	      	}" 
			mode="{device>/listMode}" 
			select="_handleSelect">
				<infoToolbar>
						<Toolbar id="filterToolbar" active="false" visible="false">
							<content>
								<Label id="visualizationInfo"/>
								<ToolbarSpacer/>
								<core:Icon src="sap-icon://building"/>
							</content>
						</Toolbar>
					</infoToolbar>
				<ObjectListItem id="MAIN_LIST_ITEM" press="_handleItemPress"
					title="{parts:[{path:'INDICATOR_TITLE'},{path:'TITLE'}], formatter:'.formatEvaluationHeader'}"
                    number="{ path:'CHIPS_COUNT', formatter:'.formatChipCount'}"
                    numberUnit="{path:'CHIPS_COUNT',formatter:'.formatTile'}"
                    dependents="{path:'TAGS'}">
					<attributes>
						<ObjectAttribute id="ATTR1" visible="{SB_APP_SETTING>/ID_VISIBLE}" text="ID: {path:'ID'}" />
					</attributes>
					<dependents>
							<Label text="{ path:'TAG'}" />
						</dependents>
				</ObjectListItem>
			</List>
		</content>
	</Page>
</core:View>