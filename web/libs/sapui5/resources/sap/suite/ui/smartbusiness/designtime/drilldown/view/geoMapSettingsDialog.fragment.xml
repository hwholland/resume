<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout">
	<Dialog title="{i18n>GEOMAP_SETTING}" contentHeight="40%"
		id="geoMapSettingsDialog" contentWidth="25%" horizontalScrolling='false'
		verticalScrolling='true'>
		<content>
			<l:Grid id="displaySelectedParametersGrid" defaultSpan="L12 M12 S12"
				class="displaySelectedParametersGrid">
				<l:content>
					<Label text="{i18n>WHAT_ARE_YOU_MEASURING}" class="chartTypeConfigTitle"
						required='true'>
						<layoutData>
							<l:GridData span="L12 M12 S12" />
						</layoutData>
					</Label>
					<RadioButton id="geoCentricRadioButton" groupName="1"
						text="{i18n>GEO_CENTRIC}" selected="true" select="geoMaptypeChanged"/>
					<RadioButton id="objectCentricRadioButton" groupName="1"
						text="{i18n>OBJECT_CENTRIC}" select="geoMaptypeChanged"/>
					<VBox id="geoMapdimensionMeasureSettings">
						<Label text="{i18n>OVERLAY_TYPE}" class="chartTypeConfigTitle"
							required='true'>
							<layoutData>
								<l:GridData span="L12 M12 S12" />
							</layoutData>
						</Label>
						<Select id="overlayType" class="chartTypeConfigTitle"
							width="90%" change="handleOverLayTypeChange" items="{SB_DDACONFIG>/GEO_MAP_TYPES}"
							selectedKey="{SB_DDACONFIG>/GEO_MAP_TYPE}">
							<core:Item key="{path:'SB_DDACONFIG>key'}" text="{path:'SB_DDACONFIG>text'}" />
							<layoutData>
								<l:GridData span="L9 M9 S9" />
							</layoutData>
						</Select>
						<Label id="pieDimensionLbl" text="{i18n>GEO_DIMENSION}" class="chartTypeConfigTitle"
							required='true' visible="false">
							<layoutData>
								<l:GridData span="L12 M12 S12" />
							</layoutData>
						</Label>
						<Select id="geoDimension" items="{SB_DDACONFIG>/items}"
							class="chartTypeConfigTitle" width="90%" visible="false"
							>
							<core:Item key="{path:'SB_DDACONFIG>NAME'}" text="{path:'SB_DDACONFIG>NAME'}" />
							<layoutData>
								<l:GridData span="L9 M9 S9" />
							</layoutData>
						</Select>
						<Label text="{i18n>SEL_MEASURES_TO_UPDATE}" class="chartTypeConfigTitle"
							required='true'>
							<layoutData>
								<l:GridData span="L12 M12 S12" />
							</layoutData>
						</Label>
						<Select id="aggregateMeasure" items="{SB_DDACONFIG>/items}"
							class="chartTypeConfigTitle" width="90%">
							<core:Item key="{path:'SB_DDACONFIG>NAME'}" text="{path:'SB_DDACONFIG>NAME'}" />
							<layoutData>
								<l:GridData span="L9 M9 S9" />
							</layoutData>
						</Select>
					</VBox>
					<VBox id="objectCentricgeoMapdimensionMeasureSettings"
						visible="false">
						<Label text="{i18n>OVERLAY_TYPE}" class="chartTypeConfigTitle"
							required='true'>
							<layoutData>
								<l:GridData span="L12 M12 S12" />
							</layoutData>
						</Label>
						<Select id="overlayTypeObjectCentric" class="chartTypeConfigTitle"
							width="90%" change="handleOverLayTypeChange" items="{SB_DDACONFIG>/GEO_MAP_TYPES}"
							selectedKey="{SB_DDACONFIG>/GEO_MAP_TYPE}">
							<core:Item key="{path:'SB_DDACONFIG>key'}" text="{path:'SB_DDACONFIG>text'}" />
							<layoutData>
								<l:GridData span="L9 M9 S9" />
							</layoutData>
						</Select>
						<Label text="{i18n>SEL_MEASURES_CORRESPONDING_GEO}" class="chartTypeConfigTitle"
							required='true'>
							<layoutData>
								<l:GridData span="L12 M12 S12" />
							</layoutData>
						</Label>
						<Select id="objectCentricMeasure" items="{SB_DDACONFIG>/items}"
							class="chartTypeConfigTitle" width="90%">
							<core:Item key="{path:'SB_DDACONFIG>NAME'}" text="{path:'SB_DDACONFIG>NAME'}" />
							<layoutData>
								<l:GridData span="L9 M9 S9" />
							</layoutData>
						</Select>
						<Select id="ObjectCentricGeoLocation" items="{SB_DDACONFIG>/items}"
							class="chartTypeConfigTitle" width="90%">
							<core:Item key="{path:'SB_DDACONFIG>NAME'}" text="{path:'SB_DDACONFIG>NAME'}" />
							<layoutData>
								<l:GridData span="L9 M9 S9" />
							</layoutData>
						</Select>
					</VBox>
				</l:content>
			</l:Grid>

			<!-- <List id="thresholdMeasureList" visible="false" /> -->


		</content>
		<beginButton>
			<Button text="{i18n>OK}" press="onGeoMapSettingsDialogOK" />
		</beginButton>
		<endButton>
			<Button text="{i18n>CANCEL}" press="onGeoMapSettingsDialogCancel" />
		</endButton>
	</Dialog>
</core:FragmentDefinition>