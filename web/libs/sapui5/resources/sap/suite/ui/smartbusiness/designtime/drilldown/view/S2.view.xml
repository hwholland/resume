<core:View xmlns:core="sap.ui.core" xmlns="sap.m"
	controllerName="sap.suite.ui.smartbusiness.designtime.drilldown.view.S2">
	<Page id="page" title="{i18n>ALL_ACTIVE_EVALUATIONS}">		
	<content>
			<List id="list"
				items="{
				path: '/EVALUATIONS_MODELER',
				sorter: [{
	        		path: 'MANUAL_ENTRY',
	        		descending: true,
	        		group: '.formatIndicatorId'},
	        		{
	      			path:'CHANGED_ON', 
	      			descending: true
	      		},true]
	        		
	      	}"
				mode="{device>/listMode}" select="_handleSelect">
				<ObjectListItem id="MAIN_LIST_ITEM" type="{device>/listItemType}"
					press="_handleItemPress" title="{parts:[{path:'INDICATOR_TITLE'},{path:'TITLE'}], formatter:'.formatEvaluationHeader'}" number="{VIEWS_COUNT}" numberUnit="{i18n>VIEWS}">
					<attributes>
						<ObjectAttribute id="ATTR1" visible="{SB_APP_SETTING>/ID_VISIBLE}" text="ID: {path:'ID'}" />
					</attributes>
				</ObjectListItem>
			</List>
		</content>
	</Page>
</core:View>