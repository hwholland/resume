<core:FragmentDefinition id="tiles" xmlns:core="sap.ui.core"
	xmlns:l="sap.ui.layout" xmlns:form="sap.ui.layout.form" xmlns:sb="sap.suite.ui.smartbusiness"
	xmlns:suite="sap.suite.ui.commons" xmlns="sap.m">

	<suite:ComparisonChart id="compchart" scale="M">
		<suite:data>
			<suite:ComparisonData title="{i18n>VALUE_1}"
				value="1550" />
			<suite:ComparisonData title="{i18n>VALUE_2}"
				value="219.2" />
			<suite:ComparisonData title="{i18n>VALUE_3}"
				value="66.46" />
		</suite:data>
	</suite:ComparisonChart>

	<suite:BulletChart id="bulletchart" scale="M"
		minValue="0" maxvalue="312" targetValue="150">
		<suite:actual>
			<suite:BulletChartData value="312" color="Error" />
		</suite:actual>
		<suite:thresholds>
			<suite:BulletChartData value="312" color="Error" />
			<suite:BulletChartData value="200" color="Critical" />
		</suite:thresholds>
	</suite:BulletChart>
	<suite:ComparisonChart id="compchartmul" scale="M">
		<suite:data>
			<suite:ComparisonData title="{i18n>MEASURE_1}"
				value="34" color="Good" />
			<suite:ComparisonData title="{i18n>MEASURE_2}"
				value="125" color="Error" />
			<suite:ComparisonData title="{i18n>MEASURE_3}"
				value="97" color="Critical" />
		</suite:data>
	</suite:ComparisonChart>
	<suite:MicroAreaChart id="areachart" width="164px" height="74px"
		minXValue="0" maxXValue="100" minYValue="0" maxYValue="100">
		<suite:firstXLabel>
			<suite:MicroAreaChartLabel label="{i18n>JAN_01}"
				color="Neutral" />
		</suite:firstXLabel>
		<suite:lastXLabel>
			<suite:MicroAreaChartLabel label="{i18n>JAN_31}"
				color="Neutral" />
		</suite:lastXLabel>
		<suite:firstYLabel>
			<suite:MicroAreaChartLabel label="3 M"
				color="Error" />
		</suite:firstYLabel>
		<suite:lastYLabel>
			<suite:MicroAreaChartLabel label="23 M"
				color="Good" />
		</suite:lastYLabel>
		<suite:target>
			<suite:MicroAreaChartItem>
				<suite:points>
					<suite:MicroAreaChartPoint x="0" y="0" />
					<suite:MicroAreaChartPoint x="30" y="30" />
					<suite:MicroAreaChartPoint x="60" y="40" />
					<suite:MicroAreaChartPoint x="100" y="90" />
				</suite:points>
			</suite:MicroAreaChartItem>
		</suite:target>
		<suite:innerMinThreshold>
			<suite:MicroAreaChartItem color="Good" />
		</suite:innerMinThreshold>
		<suite:innerMaxThreshold>
			<suite:MicroAreaChartItem color="Good" />
		</suite:innerMaxThreshold>
		<suite:minThreshold>
			<suite:MicroAreaChartItem color="Error">
				<suite:points>
					<suite:MicroAreaChartPoint x="0" y="0" />
					<suite:MicroAreaChartPoint x="30" y="20" />
					<suite:MicroAreaChartPoint x="60" y="30" />
					<suite:MicroAreaChartPoint x="100" y="70" />
				</suite:points>
			</suite:MicroAreaChartItem>
		</suite:minThreshold>
		<suite:maxThreshold>
			<suite:MicroAreaChartItem color="Good">
				<suite:points>
					<suite:MicroAreaChartPoint x="0" y="0" />
					<suite:MicroAreaChartPoint x="30" y="40" />
					<suite:MicroAreaChartPoint x="60" y="50" />
					<suite:MicroAreaChartPoint x="100" y="100" />
				</suite:points>
			</suite:MicroAreaChartItem>
		</suite:maxThreshold>
		<suite:chart>
			<suite:MicroAreaChartItem>
				<suite:points>
					<suite:MicroAreaChartPoint x="0" y="0" />
					<suite:MicroAreaChartPoint x="30" y="20" />
					<suite:MicroAreaChartPoint x="60" y="20" />
					<suite:MicroAreaChartPoint x="100" y="80" />
				</suite:points>
			</suite:MicroAreaChartItem>
		</suite:chart>
	</suite:MicroAreaChart>



</core:FragmentDefinition>
                                                              