<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout">
	<Dialog title="{i18n>CHOROPLETH_MAP_SETTING}" contentHeight="30%" id="choroplethSettingsDialog"
		contentWidth="25%" horizontalScrolling='false' verticalScrolling='true'>
		<content>
			<l:Grid id="displaySelectedParametersGrid" defaultSpan="L12 M12 S12" class="displaySelectedParametersGrid">
				<l:content>
					<Label text="{i18n>MAIN_MEASURE}" class="chartTypeConfigTitle"
						required='true'>
						<layoutData>
							<l:GridData span="L12 M12 S12" />
						</layoutData>
					</Label>
					<Select id="choroplethKpiMeasure" class="chartTypeConfigTitle"
						width="90%" change="handleMainMeasureChange" items="{SB_DDACONFIG>/items}"
						selectedKey="{SB_DDACONFIG>/MAIN_MEASURE}">
						<core:Item key="{path:'SB_DDACONFIG>NAME'}" text="{path:'SB_DDACONFIG>NAME'}" />
						<layoutData>
							<l:GridData span="L9 M9 S9" />
						</layoutData>
					</Select>
					<Button id = "kpiMeasureHelp" icon="sap-icon://sys-help" type="Transparent" press="helpPopOver">
						<layoutData>
							<l:GridData span="L3 M3 S3" />
						</layoutData>
					</Button>
					<!-- <Label text="{i18n>GEO_DIMENSION}" class="chartTypeConfigTitle"
						required='true'>
						<layoutData>
							<l:GridData span="L12 M12 S12" />
						</layoutData>
					</Label>
					<Select id="choroplethGeoDimension" class="chartTypeConfigTitle"
						width="90%" items="{SB_DDACONFIG>/items}">
						<core:Item key="{path:'SB_DDACONFIG>NAME'}" text="{path:'SB_DDACONFIG>NAME'}" />
						<layoutData>
							<l:GridData span="L9 M9 S9" />
						</layoutData>
					</Select>
					<Button id="geoDimensionHelp" icon="sap-icon://sys-help" type="Transparent" press="helpPopOver">
						<layoutData>
							<l:GridData span="L3 M3 S3" />
						</layoutData>
					</Button> -->
					<Label text="{i18n>THRESHOLD_MEASURE}" class="chartTypeConfigTitle">
						<layoutData>
							<l:GridData span="L12 M12 S12" />
						</layoutData>
					</Label>

					 <!-- <Input id="choroplethThresholdMeasure" class="chartTypeConfigTitle"
						showValueHelp="true" width="90%" valueHelpRequest="chooseMeasureForThreshold"
						value="{SB_DDACONFIG>/THRESHOLD_MEASURE}" valueHelpOnly="true">
						<layoutData>
							<l:GridData span="L9 M9 S9" />
						</layoutData>
					</Input> -->
					
 					<Select id="choroplethThresholdMeasure" items="{SB_DDACONFIG>/items}" class="chartTypeConfigTitle" width="90%"
 					selectedKey="{SB_DDACONFIG>/THRESHOLD_MEASURE}">
					<core:Item key="{path:'SB_DDACONFIG>NAME'}" text="{path:'SB_DDACONFIG>NAME'}" />
						<layoutData>
							<l:GridData span="L9 M9 S9" />
						</layoutData>
					</Select>
					
					<Button id="thresholdMeasureHelp" icon="sap-icon://sys-help" type="Transparent" press="helpPopOver">
						<layoutData>
							<l:GridData span="L3 M3 S3" />
						</layoutData>
					</Button>
				</l:content>
			</l:Grid>

			<!-- <List id="thresholdMeasureList" visible="false" /> -->


		</content>
		<beginButton>
			<Button text="{i18n>OK}" press="onChoroplethSettingsOk" />
		</beginButton>
		<endButton>
			<Button text="{i18n>CANCEL}" press="onChoroplethSettingsCancel" />
		</endButton>
	</Dialog>
</core:FragmentDefinition>