<core:View xmlns:core="sap.ui.core"
	xmlns:dda="sap.suite.ui.smartbusiness.designtime.drilldown.lib"
	xmlns:drilldown="sap.suite.ui.smartbusiness.lib" xmlns="sap.m"
	xmlns:viz="sap.viz.ui5.controls" xmlns:charts="sap.ca.ui.charts"
	xmlns:suite="sap.suite.ui.commons" xmlns:l="sap.ui.layout"
	controllerName="sap.suite.ui.smartbusiness.designtime.drilldown.view.S3">
	<Page class="drilldownConfiguratorPage" enableScrolling="false">
		<!-- <content> <ObjectHeader title="{TITLE}"> <attributes> <ObjectAttribute 
			id="orderOperation" text="{ID}" /> </attributes> </ObjectHeader> <StandardTile 
			id="xyz" removable='false' press="lauchConfigurator" icon="sap-icon://dimension" 
			title= "SmartBusiness Generic Drilldown" info="Configured"> </StandardTile> 
			</content> -->
		<content>
			<l:FixFlex class="ofixFlexVert">
				<l:fixContent>
					<drilldown:Filter id='ddaFilter' mode='designTime'
						suppressAction='true' />
					<ObjectHeader id="header-ribbon"
						number="{path:'SB_DDACONFIG>/CONFIG/SAP_AGGREGATE_VALUE',
		    	formatter:'.headerNumberFormatter'}"
						title="{TITLE}"
						numberUnit="{path:'SB_DDACONFIG>/CONFIG/SAP_AGGREGATE_VALUE',
		    	formatter:'.headerNumberUnitFormatter'}"
						class="header"></ObjectHeader>

					<suite:HeaderContainer id='tileContainer'
						class="tilesHeaderContainer">
					</suite:HeaderContainer>
					<HBox id="viewOptions" justifyContent="SpaceBetween">
						<items>
							<HBox id="viewNames"></HBox>
						</items>

					</HBox>
					
					<VBox id="choropleth_select_vbox" visible="false">
						<items>
							<Select id="choroplethViewSelect" items="{SB_DDACONFIG>/ALL_VIEWS}"
								change="onVizViewChange">
								<items>
									<core:Item key="{SB_DDACONFIG>ID}" text="{SB_DDACONFIG>TITLE}" />
								</items>
							</Select>
							<VBox id="choropleth_vbox" />
						</items>
					</VBox>
				</l:fixContent>

				<l:flexContent>
					<l:FixFlex id="viz_vbox" class="viz_vbox" fixContentSize="0px"
						visible="true">
						<l:fixContent>
						</l:fixContent>
						<l:flexContent>
							<suite:ChartContainer id="vizChartContainer"
								class="vizChartContainer" showFullScreen="true"
								showPersonalization="false" autoAdjustHeight="true" showLegend="false">
								<suite:dimensionSelectors>
									<Select id="vizViewSelector" items="{SB_DDACONFIG>/ALL_VIEWS}"
										change="onVizViewChange">
										<items>
											<core:Item key="{SB_DDACONFIG>ID}" text="{SB_DDACONFIG>TITLE}" />
										</items>
									</Select>
								</suite:dimensionSelectors>
								<suite:content>
								</suite:content>
							</suite:ChartContainer>
						</l:flexContent>
					</l:FixFlex>
				</l:flexContent>
			</l:FixFlex>
		</content>

	</Page>
</core:View>