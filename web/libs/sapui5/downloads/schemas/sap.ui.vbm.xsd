<!--type definition for UIElement 'sap.ui.vbm.GeoMap'-->
<xsd:complexType name="_GeoMapType">
<xsd:complexContent>
<xsd:extension base="n3:_VBIType">
<xsd:sequence>
<!--default aggregation 'vos'-->
<xsd:element ref="n3:VoAggregation" minOccurs="0" maxOccurs="unbounded"/>
<xsd:element name="vos" minOccurs="0">
<xsd:annotation>
<xsd:documentation>Aggregation of visual object types.</xsd:documentation>
</xsd:annotation>
<xsd:complexType>
<xsd:sequence>
<xsd:element ref="n3:VoAggregation" minOccurs="0" maxOccurs="unbounded"/>
</xsd:sequence>
</xsd:complexType>
</xsd:element>
<xsd:element name="resources" minOccurs="0">
<xsd:annotation>
<xsd:documentation>Aggregation of resources. The images of different spots have to be provided in the resources section. Binary resources data like images is pecified as a base64 encoded text.</xsd:documentation>
</xsd:annotation>
<xsd:complexType>
<xsd:sequence>
<xsd:element ref="n3:Resource" minOccurs="0" maxOccurs="unbounded"/>
</xsd:sequence>
</xsd:complexType>
</xsd:element>
<xsd:element name="legend" minOccurs="0">
<xsd:annotation>
<xsd:documentation>Legend for the Map</xsd:documentation>
</xsd:annotation>
<xsd:complexType>
<xsd:sequence>
<xsd:element ref="n3:Legend" minOccurs="0"/>
</xsd:sequence>
</xsd:complexType>
</xsd:element>
</xsd:sequence>
<xsd:attribute name="mapConfiguration" type="xsd:string">
<xsd:annotation>
<xsd:documentation>This is the map configuration for the geo map. The map configuration defines the used maps, the layering of the maps and the servers that can be used to request the map tiles.</xsd:documentation>
</xsd:annotation>
</xsd:attribute>
<xsd:attribute name="legendVisible" type="xsd:boolean">
<xsd:annotation>
<xsd:documentation>Toggles the visibility of the legend</xsd:documentation>
</xsd:annotation>
</xsd:attribute>
<xsd:attribute name="scaleVisible" type="xsd:boolean">
<xsd:annotation>
<xsd:documentation>Toggles the visibility of the scale. Only supported on initialization!</xsd:documentation>
</xsd:annotation>
</xsd:attribute>
<xsd:attribute name="navcontrolVisible" type="xsd:boolean">
<xsd:annotation>
<xsd:documentation>Toggles the visibility of the navigation control. Only supported on initialization!</xsd:documentation>
</xsd:annotation>
</xsd:attribute>
<xsd:attribute name="initialPosition" type="xsd:string">
<xsd:annotation>
<xsd:documentation>Initial position of the Map. Only supported on initialization!
Format is "&lt;longitude&gt;;&lt;latitude&gt;;0".</xsd:documentation>
</xsd:annotation>
</xsd:attribute>
<xsd:attribute name="initialZoom" type="xsd:string">
<xsd:annotation>
<xsd:documentation>Initial zoom. Value needs to be positive whole number. Only supported on initialization!</xsd:documentation>
</xsd:annotation>
</xsd:attribute>
<xsd:attribute name="click" type="xsd:string">
<xsd:annotation>
<xsd:documentation>Raised when the map is clicked.</xsd:documentation>
</xsd:annotation>
</xsd:attribute>
<xsd:attribute name="contextMenu" type="xsd:string">
<xsd:annotation>
<xsd:documentation>Raised when the map is right clicked/longPress(tap and hold).</xsd:documentation>
</xsd:annotation>
</xsd:attribute>
<xsd:attribute name="drop" type="xsd:string">
<xsd:annotation>
<xsd:documentation>Raised when something is dropped on the map.</xsd:documentation>
</xsd:annotation>
</xsd:attribute>
</xsd:extension>
</xsd:complexContent>
</xsd:complexType>
