<xsd:sequence>
<xsd:element ref="n3:DropTarget" minOccurs="0" maxOccurs="unbounded"/>
</xsd:sequence>
</xsd:complexType>
</xsd:element>
</xsd:sequence>
<xsd:attribute name="position" type="xsd:string">
<xsd:annotation>
<xsd:documentation>The position array of the route.</xsd:documentation>
</xsd:annotation>
</xsd:attribute>
<xsd:attribute name="color" type="xsd:string">
<xsd:annotation>
<xsd:documentation>The color of the route.</xsd:documentation>
</xsd:annotation>
</xsd:attribute>
<xsd:attribute name="start" type="xsd:string">
<xsd:annotation>
<xsd:documentation>The start point type of the route.  0: no startpoint 1: arrow as startpoint</xsd:documentation>
</xsd:annotation>
</xsd:attribute>
<xsd:attribute name="end" type="xsd:string">
