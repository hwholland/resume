<!--type definition for UIElement 'sap.ui.vbm.Spot'-->
<xsd:complexType name="_SpotType">
<xsd:complexContent>
<xsd:extension base="n3:_VoBaseType">
<xsd:sequence>
<xsd:element name="dragSource" minOccurs="0">
<xsd:annotation>
<xsd:documentation>DragSource aggregation</xsd:documentation>
</xsd:annotation>
<xsd:complexType>
<xsd:sequence>
<xsd:element ref="n3:DragSource" minOccurs="0" maxOccurs="unbounded"/>
</xsd:sequence>
</xsd:complexType>
</xsd:element>
<xsd:element name="dropTarget" minOccurs="0">
<xsd:annotation>
<xsd:documentation>DropTarget aggregation</xsd:documentation>
</xsd:annotation>
<xsd:complexType>
<xsd:sequence>
<xsd:element ref="n3:DropTarget" minOccurs="0" maxOccurs="unbounded"/>
</xsd:sequence>
</xsd:complexType>
</xsd:element>
</xsd:sequence>
<xsd:attribute name="position" type="xsd:string">
<xsd:annotation>
<xsd:documentation>The position of the spot.</xsd:documentation>
</xsd:annotation>
</xsd:attribute>
<xsd:attribute name="text" type="xsd:string">
<xsd:annotation>
<xsd:documentation>The text that is displayed in the spot. The text should not exceed  a few characters.</xsd:documentation>
</xsd:annotation>
</xsd:attribute>
<xsd:attribute name="image" type="xsd:string">
<xsd:annotation>
<xsd:documentation>The image for the spot. This must be a reference to a resource.</xsd:documentation>
</xsd:annotation>
</xsd:attribute>
<xsd:attribute name="alignment" type="xsd:string">
<xsd:annotation>
<xsd:documentation>Alignment of the element to its Position:
0: center
1: top center
2: top right
3: center right
4: bottom right
5: bottom center
6: bottom left
7: center left
8: top left</xsd:documentation>
</xsd:annotation>
</xsd:attribute>
<xsd:attribute name="scale" type="xsd:string">
<xsd:annotation>
<xsd:documentation>The scale of the spot.</xsd:documentation>
</xsd:annotation>
</xsd:attribute>
<xsd:attribute name="click" type="xsd:string">
<xsd:annotation>
<xsd:documentation>The event is raised when there is a click action on a Spot.</xsd:documentation>
</xsd:annotation>
</xsd:attribute>
