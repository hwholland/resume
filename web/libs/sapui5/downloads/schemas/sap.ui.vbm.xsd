<!--type definition for UIElement 'sap.ui.vbm.Resource'-->
<xsd:complexType name="_ResourceType">
<xsd:complexContent>
<xsd:extension base="n0:_ElementType">
<xsd:attribute name="value" type="xsd:string">
<xsd:annotation>
<xsd:documentation>Value of the resource.</xsd:documentation>
</xsd:annotation>
</xsd:attribute>
<xsd:attribute name="src" type="xsd:string">
<xsd:annotation>
<xsd:documentation>URL to an image. It is important that the image data is readable fom the visual business control. Therefore e.g. images coming from a local drive or cross domains are not allowed. The preferred way is to use the base 64 encoded data provided using the value property.</xsd:documentation>
</xsd:annotation>
</xsd:attribute>
<xsd:attribute name="name" type="xsd:string">
<xsd:annotation>
<xsd:documentation>Name of the resource. The name should be always 
used when a resource is referenced.</xsd:documentation>
</xsd:annotation>
</xsd:attribute>
</xsd:extension>
</xsd:complexContent>
</xsd:complexType>
<!--type definition for UIElement 'sap.ui.vbm.Route'-->
<xsd:complexType name="_RouteType">
<xsd:complexContent>
<xsd:extension base="n3:_VoBaseType">
<xsd:sequence>
<xsd:element name="dragSource" minOccurs="0">
<xsd:annotation>
<xsd:documentation>DragSource aggregation</xsd:documentation>
</xsd:annotation>
