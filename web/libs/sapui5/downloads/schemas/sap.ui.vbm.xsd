<!--type definition for UIElement 'sap.ui.vbm.Container'-->
<xsd:complexType name="_ContainerType">
<xsd:complexContent>
<xsd:extension base="n3:_VoBaseType">
<xsd:sequence>
<xsd:element name="item" minOccurs="0">
<xsd:annotation>
<xsd:documentation>The control that should be placed in the container.</xsd:documentation>
</xsd:annotation>
<xsd:complexType>
<xsd:sequence>
<xsd:choice minOccurs="0">
<xsd:element ref="n0:Control" minOccurs="0"/>
<xsd:any processContents="lax" namespace="http://www.w3.org/1999/xhtml"/>
</xsd:choice>
</xsd:sequence>
</xsd:complexType>
</xsd:element>
</xsd:sequence>
<xsd:attribute name="position" type="xsd:string">
<xsd:annotation>
<xsd:documentation>The position array for the Container.</xsd:documentation>
</xsd:annotation>
</xsd:attribute>
<xsd:attribute name="alignment" type="xsd:string">
<xsd:annotation>
<xsd:documentation>Alignment of the element to its Position:
0: center
1: top center
2: top right
3: center right
4: bottom right
5: bottom center
6: bottom left
7: center left
8: top left</xsd:documentation>
</xsd:annotation>
</xsd:attribute>
<xsd:attribute name="click" type="xsd:string">
<xsd:annotation>
<xsd:documentation>The event is raised when there is a click action on a Container.</xsd:documentation>
</xsd:annotation>
</xsd:attribute>
<xsd:attribute name="contextMenu" type="xsd:string">
<xsd:annotation>
<xsd:documentation>The event is raised when there is a  right click or a tap and hold action on a Container.</xsd:documentation>
</xsd:annotation>
</xsd:attribute>
</xsd:extension>
</xsd:complexContent>
</xsd:complexType>
