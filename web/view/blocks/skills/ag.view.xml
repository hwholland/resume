<mvc:View
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:l="sap.ui.layout"
	xmlns="sap.m">
	<l:BlockLayout background="Dashboard" class="sapUiLargeMarginBegin">
		<l:BlockLayoutRow>
			<l:BlockLayoutCell>
				<Title text="Technical Skills (Leveraged)"/>
			</l:BlockLayoutCell>
		</l:BlockLayoutRow>
        <l:BlockLayoutRow>
            <l:BlockLayoutCell>
							<List backgroundDesign="Transparent" items="{ path: 'tokens&gt;/ag'}">
								<items>
									<StandardListItem description="{tokens&gt;description}" info="{tokens>level}" title="{tokens&gt;text}"/>
								</items>
							</List>
						</l:BlockLayoutCell>
					</l:BlockLayoutRow>
				</l:BlockLayout>
							</mvc:View>