<core:View controllerName="resume.web.controller.Home" height="100%" width="100%" xmlns="sap.uxap" xmlns:consulting="resume.web.view.blocks.consulting"  xmlns:employment="resume.web.view.blocks.employment" xmlns:core="sap.ui.core" xmlns:entreprenuership="resume.web.view.blocks.entreprenuership" xmlns:f="sap.ui.layout.form" xmlns:layout="sap.ui.layout" xmlns:m="sap.m" xmlns:skills="resume.web.view.blocks.skills">
	<ObjectPageLayout alwaysShowContentHeader="false" id="ObjectPageLayout" useIconTabBar="true" subSectionLayout="TitleOnLeft">
		<headerTitle>
			<ObjectPageHeader headerDesign="Light" id="header" isObjectIconAlwaysVisible="true" objectImageShape="Circle" objectImageURI="./view/img/picture.jpg" objectSubtitle="Software Engineer &amp; Data Architect" objectTitle="Harrison Holland">
				<actions>
					<ObjectPageHeaderActionButton icon="sap-icon://action" text="action"/>
					<ObjectPageHeaderActionButton icon="sap-icon://action-settings" text="settings"/>
					<ObjectPageHeaderActionButton icon="sap-icon://edit" text="edit"/>
					<ObjectPageHeaderActionButton icon="sap-icon://save" text="save" visible="false"/>
					<ObjectPageHeaderActionButton icon="sap-icon://refresh" text="{buttons&gt;/text}"/>
				</actions>
			</ObjectPageHeader>
		</headerTitle>
		<headerContent>
			<layout:VerticalLayout>
				<m:ObjectStatus text="Atlanta, GA" title="Location"/>
				<m:ObjectStatus text="+1 (757) 291-9057" title="Phone"/>
				<m:ObjectAttribute active="false" text="harrisonholland@gmail.com" title="Email"/>
			</layout:VerticalLayout>
			<layout:VerticalLayout>
				<m:ObjectAttribute active="true" text="https://www.linkedin.com/in/harrisonholland" title="LinkedIn"/>
				<m:ObjectAttribute active="true" text="https://github.com/hwholland" title="GitHub"/>
				<m:ObjectAttribute active="true" text="https://people.sap.com/harrison.holland4" title="SAP Developer Network"/>
			</layout:VerticalLayout>
			<layout:HorizontalLayout>
				<m:Text text="Hello, I'm Harrison.  I have been working in the IT field for 15 years in various roles.  The areas where I have unique knowledge and experience include: abstract systems, data architecture / modeling, and digital security." width="375px"/>
			</layout:HorizontalLayout>
		</headerContent>
		<sections>
			<ObjectPageSection title="Startups">
				<subSections>
					<ObjectPageSubSection title="SOLO">
						<blocks>
							<entreprenuership:solo columnLayout="auto"/>
						</blocks>
						<moreBlocks>
							<m:List backgroundDesign="Transparent" class="sapUiLargeMarginBegin" items="{ path: 'tokens&gt;/solo' }">
								<m:items>
									<m:StandardListItem info="{tokens&gt;info}" title="{tokens&gt;text}"/>
								</m:items>
							</m:List>
						</moreBlocks>
					</ObjectPageSubSection>
					<ObjectPageSubSection title="Holland Systems / Big Concepts">
						<blocks>
							<entreprenuership:concepts columnLayout="auto"/>
						</blocks>
						<moreBlocks>
							<m:List backgroundDesign="Transparent" class="sapUiLargeMarginBegin" items="{ path: 'tokens&gt;/concepts'}">
								<m:items>
									<m:StandardListItem info="{tokens&gt;info}" title="{tokens&gt;text}"/>
								</m:items>
							</m:List>
						</moreBlocks>
					</ObjectPageSubSection>
					<ObjectPageSubSection title="Discrete Ventures">
						<blocks>
							<entreprenuership:social columnLayout="auto"/>
						</blocks>
						<moreBlocks>
							<m:Tokenizer enableMultiLineMode="true" tokens="{ path: 'tokens&gt;/discrete' }">
								<m:tokens>
									<m:Token editable="false" key="{tokens&gt;key}" text="{tokens&gt;text}"/>
								</m:tokens>
							</m:Tokenizer>
						</moreBlocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection title="Consulting">
