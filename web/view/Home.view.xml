					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection title="Employment">
				<subSections>
					<ObjectPageSubSection title="SAP America">
											<blocks>
							<employment:sap columnLayout="auto"/>
						</blocks>
					</ObjectPageSubSection>
					<ObjectPageSubSection title="Wolseley">
