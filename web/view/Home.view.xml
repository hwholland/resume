						<moreBlocks>
							<skills:ko columnLayout="auto"/>
						</moreBlocks>
					</ObjectPageSubSection>
					<ObjectPageSubSection title="Mylan Pharmaceuticals">
						<blocks>
							<consulting:mylan columnLayout="auto"/>
