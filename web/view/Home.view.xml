<core:View controllerName="resume.web.controller.Home" height="100%" width="100%" xmlns="sap.uxap" xmlns:consulting="resume.web.view.blocks.consulting" xmlns:core="sap.ui.core" xmlns:employment="resume.web.view.blocks.employment" xmlns:entreprenuership="resume.web.view.blocks.entreprenuership" xmlns:f="sap.ui.layout.form" xmlns:layout="sap.ui.layout" xmlns:m="sap.m" xmlns:skills="resume.web.view.blocks.skills" xmlns:visualize="resume.web.view.blocks.visualize" xmlns:viz="sap.viz.ui5.controls" xmlns:viz.data="sap.viz.ui5.data" xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds">
	<ObjectPageLayout alwaysShowContentHeader="false" id="ObjectPageLayout" showTitleInHeaderContent="true" subSectionLayout="TitleOnLeft" useIconTabBar="true">
		<headerTitle>
			<ObjectPageHeader headerDesign="Light" id="header" objectImageShape="Circle" objectImageURI="./view/img/picture.jpg" objectSubtitle="Software Engineer &amp; Data Architect" objectTitle="Harrison Holland">
				<actions>
					<ObjectPageHeaderActionButton icon="sap-icon://action" text="action"/>
					<ObjectPageHeaderActionButton icon="sap-icon://action-settings" text="settings"/>
					<ObjectPageHeaderActionButton icon="sap-icon://edit" text="edit"/>
					<ObjectPageHeaderActionButton icon="sap-icon://save" text="save" visible="false"/>
					<ObjectPageHeaderActionButton icon="sap-icon://refresh" text="{buttons&gt;/text}"/>
				</actions>
			</ObjectPageHeader>
		</headerTitle>
		<headerContent>
			<layout:VerticalLayout>
				<m:ObjectStatus text="Atlanta, GA" title="Location"/>
				<m:ObjectStatus text="+1 (757) 291-9057" title="Phone"/>
				<m:ObjectAttribute active="false" text="harrisonholland@gmail.com" title="Email"/>
			</layout:VerticalLayout>
			<layout:VerticalLayout>
				<m:ObjectAttribute active="true" text="https://www.linkedin.com/in/harrisonholland" title="LinkedIn" press="onPressLinkedIn"/>
				<m:ObjectAttribute active="true" text="https://github.com/hwholland" title="GitHub" press="onPressGithub"/>
				<m:ObjectAttribute active="true" text="https://people.sap.com/harrison.holland4
					" title="SAP Developer Network" press="onPressSDN"/>
			</layout:VerticalLayout>
			<layout:HorizontalLayout>
				<m:Text text="Hello, I'm Harrison.  I have been working in the IT field for 15 years in various roles.  The areas where I have unique knowledge and experience include: abstract systems, data architecture / modeling, and digital security." width="375px"/>
			</layout:HorizontalLayout>
		</headerContent>
		<sections>
			<ObjectPageSection title="Overview">
				<subSections>
					<ObjectPageSubSection>
						<blocks>
							<layout:BlockLayout>
								<layout:BlockLayoutRow>
									<layout:BlockLayoutCell>
										<viz:VizFrame id="idVizFrame" uiConfig="{applicationSet:'fiori'}" vizType="heatmap" width="100%">
											<viz:dataset>
												<viz.data:FlattenedDataset data="{/data/heatmap}">
													<viz.data:dimensions>
														<viz.data:DimensionDefinition name="Skills" value="{skills}"/>
														<viz.data:DimensionDefinition name="Company" value="{company}"/>
													</viz.data:dimensions>
													<viz.data:measures>
														<viz.data:MeasureDefinition name="Volume" value="{level}"/>
													</viz.data:measures>
												</viz.data:FlattenedDataset>
											</viz:dataset>
											<viz:feeds>
												<viz.feeds:FeedItem type="Measure" uid="color" values="Volume"/>
												<viz.feeds:FeedItem type="Dimension" uid="categoryAxis" values="Skills"/>
												<viz.feeds:FeedItem type="Dimension" uid="categoryAxis2" values="Company"/>
											</viz:feeds>
										</viz:VizFrame>
									</layout:BlockLayoutCell>
								</layout:BlockLayoutRow>
							</layout:BlockLayout>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection title="Startups">
				<subSections>
					<ObjectPageSubSection title="Solo">
						<blocks>
							<entreprenuership:solo columnLayout="auto"/>
						</blocks>
						<moreBlocks>
							<skills:solo columnLayout="auto"/>
						</moreBlocks>
					</ObjectPageSubSection>
					<ObjectPageSubSection title="Holland Systems / Big Concepts">
						<blocks>
							<entreprenuership:concepts columnLayout="auto"/>
						</blocks>
						<moreBlocks>
							<skills:hs columnLayout="auto"/>
						</moreBlocks>
					</ObjectPageSubSection>
					<ObjectPageSubSection title="Discrete Ventures">
						<blocks>
							<entreprenuership:social columnLayout="auto"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
